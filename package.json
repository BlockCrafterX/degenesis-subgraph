{
  "name": "tokemak-subgraph",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create Tokemak/tokemak-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create Tokemak/tokemak-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy Tokemak/tokemak-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy Tokemak/tokemak-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "node": "dotenv -- cross-var ganache-cli -h 0.0.0.0 -f https://mainnet.infura.io/v3/%INFURA_KEY%",
    "test": "mocha"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "mocha": "^9.0.2",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
